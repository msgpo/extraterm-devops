{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "removeComments": false,
        "sourceMap": true,
        "noLib": false,
        "outDir": "dist",
        "jsx": "react",
        "experimentalDecorators": true,
        "baseUrl": "."
    },
    "filesGlob": [
        "./src/*.ts"
    ],
    "exclude": []
}
